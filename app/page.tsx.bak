'use client';

import { useState, useEffect, useCallback } from 'react';
import { Dialog, Transition } from '@headlessui/react';
import { Fragment } from 'react';
import {
  HomeIcon,
  PlusIcon,
  ShoppingCartIcon,
  ChartBarIcon,
  XMarkIcon } from
'@heroicons/react/24/outline';
import { useForm } from 'react-hook-form';
import { zodResolver } from '@hookform/resolvers/zod';
import { z } from 'zod';
import { create } from 'zustand';
import { persist } from 'zustand/middleware';
import { format } from 'date-fns';

interface Product {
  id: string;
  name: string;
  barcode: string;
  category: string;
  quantity: number;
  minQuantity: number;
  expiryDate?: string;
  createdAt: string;
}

interface AppState {
  products: Product[];
  shoppingList: string[];
  addProduct: (product: Omit<Product, 'id' | 'createdAt'>) => void;
  updateProduct: (id: string, updates: Partial<Product>) => void;
  deleteProduct: (id: string) => void;
  addToShoppingList: (productName: string) => void;
  removeFromShoppingList: (productName: string) => void;
}

const useStore = create<AppState>()(
  persist(
    (set, get) => ({
      products: [],
      shoppingList: [],
      addProduct: (product) => set((state) => ({
        products: [...state.products, {
          ...product,
          id: Math.random().toString(36).substr(2, 9),
          createdAt: new Date().toISOString()
        }]
      })),
      updateProduct: (id, updates) => set((state) => ({
        products: state.products.map((p) =>
        p.id === id ? { ...p, ...updates } : p
        )
      })),
      deleteProduct: (id) => set((state) => ({
        products: state.products.filter((p) => p.id !== id)
      })),
      addToShoppingList: (productName) => set((state) => ({
        shoppingList: [...new Set([...state.shoppingList, productName])]
      })),
      removeFromShoppingList: (productName) => set((state) => ({
        shoppingList: state.shoppingList.filter((item) => item !== productName)
      }))
    }),
    {
      name: 'inventory-storage'
    }
  )
);

const productSchema = z.object({
  name: z.string().min(1, 'Nombre requerido'),
  barcode: z.string().min(1, 'Código de barras requerido'),
  category: z.string().min(1, 'Categoría requerida'),
  quantity: z.number().min(0, 'Cantidad debe ser mayor o igual a 0'),
  minQuantity: z.number().min(0, 'Cantidad mínima debe ser mayor o igual a 0'),
  expiryDate: z.string().optional()
});

type ProductFormData = z.infer<typeof productSchema>;

type ModalType = 'add-product' | 'shopping-list' | 'statistics' | null;

export default function HomePage() {
  const [activeModal, setActiveModal] = useState<ModalType>(null);
  const [scannedBarcode, setScannedBarcode] = useState('');
  const [isLoading, setIsLoading] = useState(false);

  const { products, shoppingList, addProduct, addToShoppingList, removeFromShoppingList } = useStore();

  const { register, handleSubmit, reset, setValue, formState: { errors } } = useForm<ProductFormData>({
    resolver: zodResolver(productSchema)
  });

  const lowStockProducts = products.filter((p) => p.quantity <= p.minQuantity);
  const expiredProducts = products.filter((p) => p.expiryDate && new Date(p.expiryDate) < new Date());

  useEffect(() => {
    if (scannedBarcode) {
      setValue('barcode', scannedBarcode);
    }
  }, [scannedBarcode, setValue]);

  const handleScanBarcode = useCallback(() => {
    setIsLoading(true);
    setTimeout(() => {
      const mockBarcode = Math.random().toString(10).substr(2, 12);
      setScannedBarcode(mockBarcode);
      setIsLoading(false);
    }, 1500);
  }, []);

  const onSubmit = (data: ProductFormData) => {
    addProduct(data);
    reset();
    setActiveModal(null);
    setScannedBarcode('');
  };

  const navigationItems = [
  { id: 'home', icon: HomeIcon, label: 'Inicio' },
  { id: 'add-product', icon: PlusIcon, label: 'Agregar' },
  { id: 'shopping-list', icon: ShoppingCartIcon, label: 'Compras' },
  { id: 'statistics', icon: ChartBarIcon, label: 'Estadísticas' }] as
  const;

  return (
    <div className="min-h-screen bg-gradient-to-br from-cyan-500 via-blue-600 to-purple-700" data-zeus-id="Z-11">
      <main className="container mx-auto px-4 py-6" data-zeus-id="Z-12">
        <div className="bg-white/10 backdrop-blur-lg rounded-3xl p-6 shadow-2xl" data-zeus-id="Z-13" style={{ borderWidth: 2, borderColor: '#265dc9', boxShadow: '0px 22px 19px 9px rgba(0,0,0,0.31)' }}>
          <h1 className="text-3xl font-bold text-white text-center mb-8" data-zeus-id="Z-14" style={{ fontSize: 55, paintOrder: 'stroke fill', WebkitTextStroke: '4.6px #265dc9' }}>
            Mi Inventario
          </h1>

          <div className="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6 mb-8" data-zeus-id="Z-15">
            <div className="bg-white/20 rounded-2xl p-4 text-white" data-zeus-id="Z-16" style={{ borderWidth: 2, borderColor: '#265dc9', boxShadow: '0px 22px 19px 9px rgba(0,0,0,0.31)' }}>
              <h3 className="text-lg font-semibold" data-zeus-id="Z-17" style={{ color: '#ff0000', fontWeight: '800' }}>Total Productos</h3>
              <p className="text-2xl font-bold" data-zeus-id="Z-18">{products.length}</p>
            </div>
            <div className="bg-yellow-500/20 rounded-2xl p-4 text-white" data-zeus-id="Z-19" style={{ borderWidth: 2, borderColor: '#265dc9', boxShadow: '0px 22px 19px 9px rgba(0,0,0,0.31)' }}>
              <h3 className="text-lg font-semibold" data-zeus-id="Z-20" style={{ color: '#ff0000', fontWeight: '800' }}>Stock Bajo</h3>
              <p className="text-2xl font-bold" data-zeus-id="Z-21">{lowStockProducts.length}</p>
            </div>
            <div className="bg-red-500/20 rounded-2xl p-4 text-white" data-zeus-id="Z-22" style={{ borderWidth: 2, borderColor: '#265dc9', boxShadow: '0px 22px 19px 9px rgba(0,0,0,0.31)' }}>
              <h3 className="text-lg font-semibold" data-zeus-id="Z-23" style={{ color: '#ff0000', fontWeight: '800' }}>Caducados</h3>
              <p className="text-2xl font-bold" data-zeus-id="Z-24" style={{ color: '#ffffff' }}>{expiredProducts.length}</p>
            </div>
          </div>

          <div className="bg-black/20 rounded-2xl p-6 mb-8" data-zeus-id="Z-25" style={{ borderWidth: 2, borderColor: '#265dc9', boxShadow: '0px 22px 19px 9px rgba(0,0,0,0.31)' }}>
            <h2 className="text-xl font-semibold text-white mb-4" data-zeus-id="Z-26">Productos Recientes</h2>
            <div className="space-y-3" data-zeus-id="Z-27">
              {products.slice(-5).map((product) =>
              <div key={product.id} className="flex justify-between items-center bg-white/10 rounded-xl p-3" data-zeus-id="Z-28">
                  <div data-zeus-id="Z-29">
                    <p className="text-white font-medium" data-zeus-id="Z-30">{product.name}</p>
                    <p className="text-white/70 text-sm" data-zeus-id="Z-31">{product.category}</p>
                  </div>
                  <span className="text-white font-bold" data-zeus-id="Z-32">{product.quantity} unidades</span>
                </div>
              )},
  {products.length === 0 &&
              <p className="text-white/70 text-center py-4" data-zeus-id="Z-33">No hay productos agregados</p>
              }
            </div>
          </div>
        </div>

        <nav className="fixed bottom-6 left-1/2 transform -translate-x-1/2 bg-black/80 backdrop-blur-lg rounded-full p-2 shadow-2xl" data-zeus-id="Z-34">
          <div className="flex space-x-2" data-zeus-id="Z-35">
            {navigationItems.map((item) =>
            <button
              key={item.id}
              onClick={() => setActiveModal(item.id as ModalType)}
              className="flex flex-col items-center p-3 rounded-full text-white hover:bg-white/20 transition-colors" data-zeus-id="Z-36">

                <item.icon className="w-6 h-6" data-zeus-id="Z-37" />
                <span className="text-xs mt-1" data-zeus-id="Z-38">{item.label}</span>
              </button>
            )}
          </div>
        </nav>

        <AddProductModal
          isOpen={activeModal === 'add-product'}
          onClose={() => {
            setActiveModal(null);
            reset();
            setScannedBarcode('');
          }}
          onSubmit={handleSubmit(onSubmit)}
          register={register}
          errors={errors}
          onScanBarcode={handleScanBarcode}
          scannedBarcode={scannedBarcode}
          isLoading={isLoading} data-zeus-id="Z-39" />


        <ShoppingListModal
          isOpen={activeModal === 'shopping-list'}
          onClose={() => setActiveModal(null)}
          shoppingList={shoppingList}
          onAddItem={addToShoppingList}
          onRemoveItem={removeFromShoppingList} data-zeus-id="Z-40" />


        <StatisticsModal
          isOpen={activeModal === 'statistics'}
          onClose={() => setActiveModal(null)}
          products={products}
          lowStockProducts={lowStockProducts}
          expiredProducts={expiredProducts} data-zeus-id="Z-41" />

      </main>
    </div>);

}

function ShoppingListModal({
  isOpen,
  onClose,
  shoppingList,
  onAddItem,
  onRemoveItem






}: {isOpen: boolean;onClose: () => void;shoppingList: string[];onAddItem: (productName: string) => void;onRemoveItem: (productName: string) => void;}) {
  const [newItem, setNewItem] = useState('');

  const handleAddItem = (e: React.FormEvent) => {
    e.preventDefault();
    if (newItem.trim()) {
      onAddItem(newItem.trim());
      setNewItem('');
    }
  };

  return (
    <Transition.Root show={isOpen} as={Fragment} data-zeus-id="Z-42">
      <Dialog as="div" className="relative z-50" onClose={onClose} data-zeus-id="Z-43">
        <Transition.Child
          as={Fragment}
          enter="ease-out duration-300"
          enterFrom="opacity-0"
          enterTo="opacity-100"
          leave="ease-in duration-200"
          leaveFrom="opacity-100"
          leaveTo="opacity-0" data-zeus-id="Z-44">

          <div className="fixed inset-0 bg-black bg-opacity-50" data-zeus-id="Z-45" />
        </Transition.Child>

        <div className="fixed inset-0 overflow-y-auto" data-zeus-id="Z-46">
          <div className="flex min-h-full items-center justify-center p-4" data-zeus-id="Z-47">
            <Transition.Child
              as={Fragment}
              enter="ease-out duration-300"
              enterFrom="opacity-0 scale-95"
              enterTo="opacity-100 scale-100"
              leave="ease-in duration-200"
              leaveFrom="opacity-100 scale-100"
              leaveTo="opacity-0 scale-95" data-zeus-id="Z-48">

              <Dialog.Panel className="w-full max-w-md transform overflow-hidden rounded-2xl bg-white p-6 shadow-xl transition-all" data-zeus-id="Z-49">
                <div className="flex justify-between items-center mb-6" data-zeus-id="Z-50">
                  <Dialog.Title className="text-xl font-bold text-gray-900" data-zeus-id="Z-51">
                    Lista de Compras
                  </Dialog.Title>
                  <button
                    onClick={onClose}
                    className="text-gray-400 hover:text-gray-600" data-zeus-id="Z-52">

                    <XMarkIcon className="w-6 h-6" data-zeus-id="Z-53" />
                  </button>
                </div>

                <form onSubmit={handleAddItem} className="mb-6" data-zeus-id="Z-54">
                  <div className="flex space-x-2" data-zeus-id="Z-55">
                    <input
                      type="text"
                      value={newItem}
                      onChange={(e) => setNewItem(e.target.value)}
                      placeholder="Agregar producto..."
                      className="flex-1 px-3 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blue-500 focus:border-blue-500" data-zeus-id="Z-56" />

                    <button
                      type="submit"
                      className="px-4 py-2 bg-blue-600 text-white rounded-lg hover:bg-blue-700" data-zeus-id="Z-57">

                      Agregar
                    </button>
                  </div>
                </form>

                <div className="space-y-2 max-h-64 overflow-y-auto" data-zeus-id="Z-58">
                  {shoppingList.map((item, index) =>
                  <div key={index} className="flex justify-between items-center bg-gray-50 rounded-lg p-3" data-zeus-id="Z-59">
                      <span className="text-gray-900" data-zeus-id="Z-60">{item}</span>
                      <button
                      onClick={() => onRemoveItem(item)}
                      className="text-red-500 hover:text-red-700" data-zeus-id="Z-61">

                        <XMarkIcon className="w-5 h-5" data-zeus-id="Z-62" />
                      </button>
                    </div>
                  )}
                  {shoppingList.length === 0 &&
                  <p className="text-gray-500 text-center py-4" data-zeus-id="Z-63">
                      No hay productos en la lista
                    </p>
                  }
                </div>
              </Dialog.Panel>
            </Transition.Child>
          </div>
        </div>
      </Dialog>
    </Transition.Root>);

}

