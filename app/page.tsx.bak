  const categories = products.reduce((acc, product) => {
    acc[product.category] = (acc[product.category] || 0) + 1;
    return acc;
  }, {} as Record<string, number>);

  return (
    <Transition.Root show={isOpen} as={Fragment} data-zeus-id="Z-64">
      <Dialog as="div" className="relative z-50" onClose={onClose} data-zeus-id="Z-65">
