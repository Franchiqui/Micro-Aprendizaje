                <div className="space-y-4" data-zeus-id="Z-76">
                  <div className="grid grid-cols-2 gap-4" data-zeus-id="Z-77">
                    <div className="bg-blue-50 rounded-lg p-4" data-zeus-id="Z-78">
                      <p className="text-sm text-blue-600" data-zeus-id="Z-79">Total Productos</p>
                      <p className="text-2xl font-bold text-blue-900" data-zeus-id="Z-80">{products.length}</p>
                    </div>
                    <div className="bg-yellow-50 rounded-lg p-4" data-zeus-id="Z-81">
                      <p className="text-sm text-yellow-600" data-zeus-id="Z-82">Stock Bajo</p>
                      <p className="text-2xl font-bold text-yellow-900" data-zeus-id="Z-83">{lowStockProducts.length}</p>
                    </div>
                    <div className="bg-red-50 rounded-lg p-4" data-zeus-id="Z-84">
                      <p className="text-sm text-red-600" data-zeus-id="Z-85">Caducados</p>
                      <p className="text-2xl font-bold text-red-900" data-zeus-id="Z-86">{expiredProducts.length}</p>
                    </div>
                    <div className="bg-green-50 rounded-lg p-4" data-zeus-id="Z-87">
